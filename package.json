{
  "name": "@krist7599555/svelte-turing-render",
  "version": "1.2.1",
  "description": "allow svelte to run in turing machine like behavior",
  "keywords": [
    "svelte",
    "sveltekit"
  ],
  "license": "ISC",
  "author": "Krist Ponpairin <krist7599555>",
  "type": "module",
  "scripts": {
    "build:docs": "vite build",
    "build:package": "svelte-kit sync && npm version patch && svelte-package && cd package && npm publish --access public",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "devDependencies": {
    "@krist7599555/harmony-editorconfig": "^0.1.4",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "1.0.0-next.47",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.8.0",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.20",
    "tslib": "^2.4.1",
    "type-fest": "^3.1.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
  },
  "peerDependencies": {
    "type-fest": "*"
  }
}
