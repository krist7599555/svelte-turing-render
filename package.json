{
  "name": "@krist7599555/svelte-turing-render",
  "version": "2.0.0",
  "description": "allow svelte to run in turing machine like behavior",
  "keywords": [
    "svelte",
    "sveltekit"
  ],
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "license": "ISC",
  "author": "Krist Ponpairin <krist7599555>",
  "type": "module",
  "scripts": {
    "build": "svelte-package",
    "build:docs": "vite build",
    "build:package": "svelte-kit sync && npm version patch && svelte-package && cd package && npm publish --access public",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/adapter-static": "2.0.2",
    "@sveltejs/kit": "^1.22.3",
    "@sveltejs/package": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte": "^2.32.2",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "svelte2tsx": "^0.6.19",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.4"
  },
  "dependencies": {
    "svelte": "^4.0.5"
  },
  "pnpm": {
    "overrides": {
      "prettier": "$prettier"
    }
  }
}
